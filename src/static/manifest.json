{
    "manifest_version": 3,
    "name": "Mobioptions ASO Best ASO Toolbox For Google Play & App Store",
    "description": "Enhances the Google Play Storeâ„¢ & App Store with various ASO features.",
    "version": "1.0.6",
    "action" : {
        "default_popup": "popup.html",
        "default_title": "MobiOptions",
        "default_icon" : "icon.png"
    },
    "icons": {
        "16" : "icon.png",
        "48" : "icon.png",
        "128" : "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://play.google.com/store/*", "https://apps.apple.com/*"],
            "js": ["contentScript.js"]
        }
    ]
}